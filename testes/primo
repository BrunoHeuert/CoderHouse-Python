num = int(input('\nDigite um número: '))
mult = 0
for c in range(1, num + 1):
    if num % c == 0:
        print(c, end=' - ')
        mult = mult + 1
print('FIM')
print(f'O número {num} foi divisível {mult} vezes')
if mult == 2:
    print('E por isso ele é primo!')
else:
    print('E por isso não é primo!')